{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite --port 9051 --host",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:tsc": "vue-tsc",
    "build:prod": "vite build --mode production",
    "openapi:gen": "openapi --input ../resources/openapi.yaml --output ./src/models/api --client fetch",
    "fmt": "prettier . --write",
    "check": "prettier . --check",
    "test": "vitest run",
    "test:unit": "vitest && vue-tsc"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.1.0",
    "@mdi/font": "^7.4.47",
    "apexcharts": "^3.54.0",
    "core-js": "^3.29.0",
    "moment": "^2.30.1",
    "pinia": "^2.2.4",
    "roboto-fontface": "*",
    "ts-json-schema-generator": "^2.3.0",
    "vue": "^3.5.11",
    "vue-codemirror6": "^1.2.5",
    "vue-router": "^4.4.5",
    "vue3-apexcharts": "^1.7.0",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@babel/types": "^7.25.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/d3": "^7.4.3",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.4.20",
    "@vue/eslint-config-typescript": "^14.0.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "msw": "^2.4.9",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "3.3.3",
    "sass": "^1.79.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "~2.1.2",
    "vue-tsc": "^2.1.6",
    "widdershins": "^4.0.1"
  }
}
