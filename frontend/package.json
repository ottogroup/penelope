{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite --port 9051 --host",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:tsc": "vue-tsc",
    "build:prod": "vite build --mode production",
    "openapi:gen": "openapi --input ../resources/openapi.yaml --output ./src/models/api --client fetch",
    "fmt": "prettier . --write",
    "check": "prettier . --check",
    "test": "vitest run",
    "test:unit": "vitest && vue-tsc"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.8",
    "@mdi/font": "^7.4.47",
    "apexcharts": "^5.3.5",
    "core-js": "^3.45.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.3",
    "roboto-fontface": "*",
    "ts-json-schema-generator": "^2.4.0",
    "vue": "^3.5.22",
    "vue-codemirror6": "^1.3.22",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.4.4",
    "vuetify": "^3.10.4"
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/d3": "^7.4.3",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "msw": "^2.11.3",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "3.6.2",
    "sass": "^1.93.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "~3.2.4",
    "vue-tsc": "^3.1.1",
    "widdershins": "^4.0.1"
  }
}
